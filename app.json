{
  "expo": {
    "name": "react-native-webview-leaflet",
    "description": "This webview containing a clickable Leaflet map.",
    "slug": "react-native-webview-leaflet",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "assetExts": ["html"]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "hooks": {
      
    },
    "ios": {
      "bundleIdentifier": "com.reggie3.reactnativewebviewleaflet",
      "supportsTablet": true
    },
    "android": {
      "package": "com.reggie3.reactnativewebviewleaflet"
    }
  }
  
}
